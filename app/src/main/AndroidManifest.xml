<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ifeng_tech.treasuryyitong">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".appliction.DashApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 启动页 -->
        <activity android:name=".MainActivity" />

        <!-- 首页展示 -->
        <activity android:name=".ui.HomePageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 心跳服务 -->
        <service android:name=".service.HeartbeatService" />

        <!-- 登录/注册 -->
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.my.My_Property_Activity" />
        <activity android:name=".ui.my.Stock_Removal_Activity" />
        <activity android:name=".ui.my.Incoming_Test_Activity" />
        <activity android:name=".ui.my.MyCollectActivity" />
        <activity android:name=".ui.my.My_Warehouse_Activity" />
        <activity android:name=".ui.my.Pick_up_goods_Activity" />
        <activity android:name=".ui.my.Donation_Activity" />
        <activity android:name=".ui.my.Collect_Activity" />
        <activity android:name=".ui.Authenticate_Details_Activity"></activity>
    </application>

</manifest>