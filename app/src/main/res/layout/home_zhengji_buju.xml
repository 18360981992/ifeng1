<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginLeft="@dimen/x8"
    android:background="@drawable/item_bg"
    >

   <ImageView
       android:id="@+id/home_zhengji_img"
       android:layout_width="@dimen/x90"
       android:layout_height="@dimen/x90"
       />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/x15"
        >
        <TextView
            android:id="@+id/home_zhengji_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="世博四连体"
            android:textColor="@color/name_se"
            android:textSize="@dimen/x12"
            android:layout_marginTop="@dimen/x2"
            android:layout_marginLeft="@dimen/x3"/>
        <TextView
            android:id="@+id/home_zhengji_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="福利特寄卖商城"
            android:textSize="@dimen/x11"
            android:textColor="@color/zhuse_ziti"
            android:layout_marginTop="@dimen/x2"
            android:layout_marginLeft="@dimen/x3"/>
        <TextView
            android:id="@+id/home_zhengji_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="托管进度10/20"
            android:textSize="@dimen/x10"
            android:textColor="@color/zhuse_ziti"
            android:layout_marginTop="@dimen/x2"
            android:layout_marginLeft="@dimen/x3"
            />
    </LinearLayout>



</LinearLayout>
