<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x250"
    android:orientation="vertical"
    android:background="@drawable/common_dialog_bg"
    android:layout_margin="@dimen/x80"
    android:paddingBottom="@dimen/x10"
    >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x210">
        <TextView
            android:id="@+id/dialog_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="是否确认注销该提货单"
            android:textSize="@dimen/x28"
            android:textColor="@color/name_se"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        >
        <TextView
            android:id="@+id/dialog_queren"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="确认"
            android:textSize="@dimen/x28"
            android:textColor="@color/zhuse"
            android:gravity="center"/>
        <View
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:background="@color/zhuse"
            />
        <TextView
            android:id="@+id/dialog_quxiao"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="取消"
            android:textSize="@dimen/x28"
            android:textColor="@color/zhuse"
            android:gravity="center"/>


    </LinearLayout>

</RelativeLayout>
