<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/fuise_dabg"
    tools:context="com.ifeng_tech.treasuryyitong.ui.my.My_Given_Datail_Activity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:background="@drawable/zichan_jianbian"
        >
        <RelativeLayout
            android:id="@+id/my_Given_Datail_Fan"
            android:layout_width="@dimen/x60"
            android:layout_height="match_parent"
            android:background="@drawable/fanhui_bg"
            >
            <ImageView
                android:layout_width="@dimen/x16"
                android:layout_height="@dimen/x28"
                android:layout_alignParentBottom="true"
                android:background="@drawable/btn_back"
                android:layout_marginBottom="@dimen/x20"
                android:layout_marginLeft="@dimen/x30"
                />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="转让列表详情"
            android:textSize="@dimen/x32"
            android:textColor="@color/baise"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/x16"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/my_Given_Datail_xiangqing"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x220"
                android:layout_marginLeft="@dimen/x18"
                android:layout_marginRight="@dimen/x18"
                android:layout_marginTop="@dimen/x30"
                android:background="@drawable/zhuanzeng_xiangqing"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/my_Given_Datail_cword"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginLeft="@dimen/x30"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="编号："
                    android:textColor="@color/color_999999"
                    android:textSize="@dimen/x26" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x2"
                    android:layout_marginLeft="@dimen/x30"
                    android:layout_marginRight="@dimen/x85"
                    android:background="@color/fengouxian" />

                <TextView
                    android:id="@+id/my_Given_Datail_name"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginLeft="@dimen/x30"
                    android:layout_marginRight="@dimen/x80"
                    android:layout_weight="1.5"
                    android:gravity="center_vertical"
                    android:paddingBottom="@dimen/x35"
                    android:text=""
                    android:textColor="@color/name_se"
                    android:textSize="@dimen/x28" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/baise"
                android:layout_marginRight="@dimen/x18"
                android:layout_marginLeft="@dimen/x18"
                android:layout_marginTop="@dimen/x5"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x20"
                    android:layout_marginBottom="@dimen/x20"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center_horizontal"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="数量"
                            android:textSize="@dimen/x28"
                            android:textColor="@color/color_666666"
                            />
                        <TextView
                            android:id="@+id/my_Given_Datail_cangku"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/x26"
                            android:textColor="@color/name_se"
                            android:layout_marginTop="@dimen/x8"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center_horizontal"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="类别"
                            android:textSize="@dimen/x28"
                            android:textColor="@color/color_666666"
                            />
                        <TextView
                            android:id="@+id/my_Given_Datail_leibie"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/x26"
                            android:textColor="@color/name_se"
                            android:layout_marginTop="@dimen/x8"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center_horizontal"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="状态"
                            android:textSize="@dimen/x28"
                            android:textColor="@color/color_666666"
                            />
                        <TextView
                            android:id="@+id/my_Given_Datail_zhangtai"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textSize="@dimen/x26"
                            android:textColor="@color/name_se"
                            android:layout_marginTop="@dimen/x8"
                            />

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x2"
                    android:background="@color/fengouxian"
                    android:layout_marginLeft="@dimen/x20"
                    android:layout_marginRight="@dimen/x20"
                    />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x20"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="订单号"
                        android:textSize="@dimen/x26"
                        android:textColor="@color/color_666666"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <TextView
                        android:id="@+id/my_Given_Datail_dword"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/x26"
                        android:textColor="@color/name_se"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/x20"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x20"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="对方账户"
                        android:textSize="@dimen/x26"
                        android:textColor="@color/color_666666"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <TextView
                        android:id="@+id/my_Given_Datail_duifang"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/x26"
                        android:textColor="@color/name_se"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/x20"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x20"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="手续费"
                        android:textSize="@dimen/x26"
                        android:textColor="@color/color_666666"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <TextView
                        android:id="@+id/my_Given_Datail_shouxufei"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/x26"
                        android:textColor="@color/name_se"
                        android:layout_marginRight="@dimen/x20"
                        android:layout_alignParentRight="true"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x20"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="创建时间"
                        android:textSize="@dimen/x26"
                        android:textColor="@color/color_666666"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <TextView
                        android:id="@+id/my_Given_Datail_riqi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/x26"
                        android:textColor="@color/name_se"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/x20"
                        />
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x20"
                    android:layout_marginBottom="@dimen/x15"
                    >
                    <TextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:text="备注"
                        android:textSize="@dimen/x26"
                        android:textColor="@color/color_666666"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_weight="3"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        >
                        <TextView
                            android:id="@+id/my_Given_Datail_beizhu"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" "
                            android:textSize="@dimen/x26"
                            android:textColor="@color/name_se"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/x20"
                            android:lineSpacingExtra="@dimen/x4"
                            />
                    </RelativeLayout>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/x25"
                android:layout_marginLeft="@dimen/x18"
                android:layout_marginRight="@dimen/x18"
                android:background="@color/baise"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x40"
                    >
                    <ImageView
                        android:layout_width="@dimen/x25"
                        android:layout_height="@dimen/x29"
                        android:background="@drawable/youqing_tishi"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="友情提示"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x10"
                        />

                </LinearLayout>

                <TextView
                    android:id="@+id/my_Given_Datail_youqingtishi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/x26"
                    android:textColor="@color/color_999999"
                    android:layout_marginLeft="@dimen/x55"
                    android:layout_marginRight="@dimen/x20"
                    android:layout_marginTop="@dimen/x10"
                    android:layout_marginBottom="@dimen/x60"
                    android:lineSpacingExtra="@dimen/x4"
                    />
                
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/my_Given_Datail_anniu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/x16"
                    android:layout_alignParentBottom="true"
                    >
                    <Button
                        android:id="@+id/my_Given_Datail_tongyi"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="@dimen/x70"
                        android:text="同意"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/baise"
                        android:layout_marginLeft="@dimen/x20"
                        android:background="#698BF9"
                        android:gravity="center"
                        />
                    <Button
                        android:id="@+id/my_Given_Datail_jujue"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="@dimen/x70"
                        android:text="拒绝"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/baise"
                        android:layout_marginRight="@dimen/x20"
                        android:background="#595C63"
                        android:layout_marginLeft="@dimen/x16"
                        android:gravity="center"
                        />
                </LinearLayout>

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/my_Given_Datail_weitanchuan"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x60"
            android:background="@color/weitanchuang"
            android:gravity="center_vertical"
            android:visibility="invisible"
            >
            <ImageView
                android:id="@+id/my_Given_Datail_weitanchuan_img"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x40"
                android:layout_marginLeft="@dimen/x16"
                />
            <TextView
                android:id="@+id/my_Given_Datail_weitanchuan_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="同意成功"
                android:textColor="@color/baise"
                android:textSize="@dimen/x28"
                android:layout_marginLeft="@dimen/x16"/>

        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
