<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x110"
        android:background="@color/fuise_dabg">
        <ImageView
            android:id="@+id/entrust_Client_list_item_fuxuan"
            android:layout_width="@dimen/x40"
            android:layout_height="@dimen/x40"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/x85"
            />
        <TextView
            android:id="@+id/entrust_Client_list_item_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="北交所福利特收藏品交易平台"
            android:textSize="@dimen/x30"
            android:textColor="@color/zhuse"
            android:lineSpacingExtra="@dimen/x4"
            android:layout_marginLeft="@dimen/x20"
            android:layout_marginRight="@dimen/x112"/>
    </LinearLayout>

</LinearLayout>
