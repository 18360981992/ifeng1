<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:scrollbars="none"
    android:background="@color/fuise_dabg"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/baise"
                android:layout_marginRight="@dimen/x20"
                android:layout_marginLeft="@dimen/x20"
                android:layout_marginTop="@dimen/x68"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="藏品名称"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>
                    <EditText
                        android:id="@+id/up_goods_zhuce_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x70"
                        android:hint="请输入藏品名称"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        android:singleLine="true"
                        android:gravity="center_vertical"
                        android:background="@null"
                        />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="藏品代码"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>
                    <EditText
                        android:id="@+id/up_goods_zhuce_word"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x70"
                        android:hint="请输入藏品代码"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        android:inputType="number"
                        android:singleLine="true"
                        android:maxLength="10"
                        android:gravity="center_vertical"
                        android:background="@null"
                        />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="交货仓库"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>

                    <!--android:background="@drawable/pick_selector"-->
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        >
                        <TextView
                            android:id="@+id/up_goods_zhuce_cangku"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/x70"
                            android:hint="请选择"
                            android:textSize="@dimen/x28"
                            android:textColor="@color/name_se"
                            android:layout_centerVertical="true"
                            android:gravity="center_vertical"
                            />
                        <ImageView
                            android:id="@+id/image_jiantou"
                            android:layout_width="@dimen/x20"
                            android:layout_height="@dimen/x16"
                            android:background="@drawable/xia_jiantou"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/x30"
                            android:layout_alignParentRight="true"
                            />

                        <!--<LinearLayout-->
                        <!--android:id="@+id/up_goods_zhuce_cangku_img"-->
                        <!--android:layout_width="@dimen/x1"-->
                        <!--android:layout_height="@dimen/x1"-->
                        <!--android:layout_alignParentRight="true"-->
                        <!--android:background="@drawable/item_bg"-->
                        <!--android:layout_centerVertical="true"-->
                        <!--android:layout_marginRight="@dimen/x2"-->
                        <!--&gt;</LinearLayout>-->


                    </RelativeLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <!--android:background="@drawable/tihuo_ed_bg"-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="交割件数"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>
                    <EditText
                        android:id="@+id/up_goods_zhuce_jiaoge_jianshu"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x70"
                        android:hint="您的最大交割件数0"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        android:inputType="number"
                        android:singleLine="true"
                        android:gravity="center_vertical"
                        android:background="@null"
                        />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="交割数量"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>
                    <EditText
                        android:id="@+id/up_goods_zhuce_jiaoge_shuliang"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x70"
                        android:hint="请输入交割数量"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/color_999999"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        android:gravity="center_vertical"
                        android:inputType="number"
                        android:background="@null"
                        />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="手续费"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>

                    <TextView
                        android:id="@+id/up_goods_zhuce_shouxufei"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x70"
                        android:hint="￥0.00"
                        />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="提货日期"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        >
                        <TextView
                            android:id="@+id/up_goods_zhuce_rili"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/x70"
                            android:hint="只能在1到30个工作日内"
                            android:textSize="@dimen/x28"
                            android:textColor="@color/name_se"
                            android:layout_centerVertical="true"
                            android:gravity="center_vertical"
                            />
                        <LinearLayout
                            android:id="@+id/up_goods_zhuce_rili_img"
                            android:layout_width="@dimen/x80"
                            android:layout_height="@dimen/x65"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:gravity="center"
                            android:background="@drawable/item_bg"
                            >
                            <ImageView
                                android:layout_width="@dimen/x35"
                                android:layout_height="@dimen/x30"
                                android:background="@drawable/riqi"
                                />
                        </LinearLayout>

                    </RelativeLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x1"
                    android:background="@color/fengouxian1"
                    android:layout_marginRight="@dimen/x30"
                    android:layout_marginLeft="@dimen/x30"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x120"
                    android:gravity="center"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="业务密码"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"/>
                    <EditText
                        android:id="@+id/up_goods_zhuce_yewu_pass"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x70"
                        android:hint="请输入业务密码"
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_marginRight="@dimen/x30"
                        android:gravity="center_vertical"
                        android:inputType="numberPassword"
                        android:maxLength="6"
                        android:background="@null"
                        />
                </LinearLayout>
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/x180"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="@dimen/x16"
                    android:layout_marginTop="@dimen/x15"
                    >
                    <Button
                        android:id="@+id/up_goods_tijiao"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="@dimen/x80"
                        android:text="下一步"
                        android:textSize="@dimen/x30"
                        android:textColor="@color/baise"
                        android:layout_marginLeft="@dimen/x20"
                        android:layout_marginRight="@dimen/x20"
                        android:background="@color/daanniu"
                        android:gravity="center"
                        />
                    <!--android:background="@drawable/congzhi_bg"-->
                    <!--<Button-->
                    <!--android:id="@+id/up_goods_chongzhi"-->
                    <!--android:layout_width="0dp"-->
                    <!--android:layout_weight="1"-->
                    <!--android:layout_height="@dimen/x70"-->
                    <!--android:text="重置"-->
                    <!--android:textSize="@dimen/x28"-->
                    <!--android:textColor="@color/baise"-->
                    <!--android:layout_marginRight="@dimen/x20"-->
                    <!--android:background="@drawable/congzhi_bg"-->
                    <!--android:layout_marginLeft="@dimen/x16"-->
                    <!--android:gravity="center"-->
                    <!--/>-->
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>

</ScrollView>
