<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/wode_list_img"
        android:layout_width="@dimen/x18"
        android:layout_height="@dimen/x18"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/x13"
        />
    <TextView
        android:id="@+id/wode_list_text"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x40"
        android:text="我的资金"
        android:textSize="@dimen/x12"
        android:textColor="@color/name_se"
        android:layout_toRightOf="@id/wode_list_img"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/x10"
        android:gravity="center"
        />

    <ImageView
        android:layout_width="@dimen/x8"
        android:layout_height="@dimen/x12"
        android:background="@drawable/jiantou"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/x10"/>

</RelativeLayout>
