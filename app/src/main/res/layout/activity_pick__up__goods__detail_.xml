<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/fuise_dabg"
    tools:context="com.ifeng_tech.treasuryyitong.ui.my.tihuo.Pick_Up_Goods_Detail_Activity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:background="@drawable/zichan_jianbian"
        >
        <RelativeLayout
            android:id="@+id/pick_up_goods_detail_Fan"
            android:layout_width="@dimen/x60"
            android:layout_height="match_parent"
            android:background="@drawable/fanhui_bg"
            >
            <ImageView
                android:layout_width="@dimen/x16"
                android:layout_height="@dimen/x28"
                android:layout_alignParentBottom="true"
                android:background="@drawable/btn_back"
                android:layout_marginBottom="@dimen/x20"
                android:layout_marginLeft="@dimen/x30"
                />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提货单详情"
            android:textSize="@dimen/x32"
            android:textColor="@color/baise"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/x16"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>




    <LinearLayout
        android:id="@+id/pick_up_goods_detail_xiangqing"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x210"
        android:layout_marginTop="@dimen/x30"
        android:layout_marginRight="@dimen/x18"
        android:layout_marginLeft="@dimen/x18"
        android:background="@drawable/zhuanzeng_xiangqing"
        android:orientation="vertical"
        >

        <TextView
            android:id="@+id/pick_up_goods_detail_cword"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="编号：325894783"
            android:textSize="@dimen/x26"
            android:textColor="@color/color_999999"
            android:layout_marginLeft="@dimen/x30"
            android:gravity="center_vertical"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:background="@color/fengouxian"
            android:layout_marginRight="@dimen/x85"
            android:layout_marginLeft="@dimen/x30"
            />
        <TextView
            android:id="@+id/pick_up_goods_detail_name"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1.5"
            android:text="世博四连体"
            android:textSize="@dimen/x28"
            android:textColor="@color/name_se"
            android:layout_marginLeft="@dimen/x30"
            android:paddingBottom="@dimen/x35"
            android:layout_marginRight="@dimen/x80"
            android:gravity="center_vertical"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/baise"
        android:layout_marginRight="@dimen/x18"
        android:layout_marginLeft="@dimen/x18"
        android:layout_marginTop="@dimen/x5"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            android:layout_marginBottom="@dimen/x20"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="数量"
                    android:textSize="@dimen/x28"
                    android:textColor="@color/color_666666"
                    />
                <TextView
                    android:id="@+id/pick_up_goods_detail_shuliang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/x26"
                    android:textColor="@color/name_se"
                    android:layout_marginTop="@dimen/x8"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="手续费"
                    android:textSize="@dimen/x28"
                    android:textColor="@color/color_666666"
                    />
                <TextView
                    android:id="@+id/pick_up_goods_detail_shouxufei"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/x26"
                    android:textColor="@color/name_se"
                    android:layout_marginTop="@dimen/x8"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="状态"
                    android:textSize="@dimen/x28"
                    android:textColor="@color/color_666666"
                    />
                <TextView
                    android:id="@+id/pick_up_goods_detail_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/x26"
                    android:textColor="@color/name_se"
                    android:layout_marginTop="@dimen/x8"
                    />

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/x2"
            android:background="@color/fengouxian"
            android:layout_marginLeft="@dimen/x20"
            android:layout_marginRight="@dimen/x20"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x30">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x20"
                android:text="订单号"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/x26"
                />

            <TextView
                android:id="@+id/pick_up_goods_detail_dword"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/name_se"
                android:textSize="@dimen/x26"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x20"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="仓库"
                android:textSize="@dimen/x26"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/pick_up_goods_detail_cangku"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/x26"
                android:textColor="@color/name_se"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x20"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="注册日期"
                android:textSize="@dimen/x26"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/pick_up_goods_detail_zhuce_riqi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/x26"
                android:textColor="@color/name_se"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x20"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="提货日期"
                android:textSize="@dimen/x26"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/pick_up_goods_detail_tihuo_riqi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/x26"
                android:textColor="@color/name_se"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x20"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x20"
            android:layout_marginBottom="@dimen/x25"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="件数"
                android:textSize="@dimen/x26"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/pick_up_goods_detail_jianshu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/x26"
                android:textColor="@color/name_se"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x20"
                />
        </RelativeLayout>

