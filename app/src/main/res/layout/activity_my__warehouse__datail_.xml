<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/baise"
    tools:context="com.ifeng_tech.treasuryyitong.ui.my.My_Warehouse_Datail_Activity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:background="@drawable/zichan_jianbian"
        >
        <RelativeLayout
            android:id="@+id/my_Warehouse_Datail_Fan"
            android:layout_width="@dimen/x60"
            android:layout_height="match_parent"
            android:background="@drawable/fanhui_bg"
            >
            <ImageView
                android:layout_width="@dimen/x16"
                android:layout_height="@dimen/x28"
                android:layout_alignParentBottom="true"
                android:background="@drawable/btn_back"
                android:layout_marginBottom="@dimen/x20"
                android:layout_marginLeft="@dimen/x30"
                />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的仓库详情"
            android:textSize="@dimen/x32"
            android:textColor="@color/baise"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/x16"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x20"
        android:layout_marginBottom="@dimen/x20"
        >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x20"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="藏品名称:"
                    android:textSize="@dimen/x26"
                    android:textColor="@color/name_se"
                    />
                <TextView
                    android:id="@+id/my_Warehouse_Datail_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="世博四连体"
                    android:textSize="@dimen/x26"
                    android:textColor="@color/name_se"
                    android:layout_marginLeft="@dimen/x20"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x20"
                android:layout_marginTop="@dimen/x16"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="订单号:"
                    android:textSize="@dimen/x22"
                    android:textColor="@color/color_999999"
                    />
                <TextView
                    android:id="@+id/my_Warehouse_Datail_cword"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="3258947836343"
                    android:textSize="@dimen/x22"
                    android:textColor="@color/name_se"
                    android:layout_marginLeft="@dimen/x60"
                    />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <!--<TextView-->
                <!--android:id="@+id/my_Warehouse_Datail_xiangqing"-->
                <!--android:layout_width="@dimen/x120"-->
                <!--android:layout_height="@dimen/x36"-->
                <!--android:layout_marginRight="@dimen/x20"-->
                <!--android:background="@drawable/shangwu_kuang"-->
                <!--android:gravity="center"-->
                <!--android:text="商品详情"-->
                <!--android:textColor="@color/zhuse"-->
                <!--android:textSize="@dimen/x20"-->
                <!--android:layout_gravity="end"-->
                <!--/>-->
            <ImageView
                android:id="@+id/my_Warehouse_Datail_erweima"
                android:layout_width="@dimen/x50"
                android:layout_height="@dimen/x50"
                android:background="@drawable/erweima_lan"
                android:layout_gravity="end|center_vertical"
                android:layout_marginRight="@dimen/x20"
                />

        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x16"
        android:background="@color/fengouxian"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x24"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="可用数量"
                android:textSize="@dimen/x22"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/my_Warehouse_Datail_keyong_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2017"
                android:textSize="@dimen/x24"
                android:textColor="@color/name_se"
                android:layout_marginLeft="@dimen/x96"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x16"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="冻结数量"
                android:textSize="@dimen/x22"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/my_Warehouse_Datail_dongjie_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2017"
                android:textSize="@dimen/x24"
                android:textColor="@color/name_se"
                android:layout_marginLeft="@dimen/x96"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x16"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="总数量"
                android:textSize="@dimen/x22"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/my_Warehouse_Datail_zong_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10000"
                android:textSize="@dimen/x24"
                android:textColor="@color/name_se"
                android:layout_marginLeft="@dimen/x116"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/x16"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="预计仓储费用"
                android:textSize="@dimen/x22"
                android:textColor="@color/color_999999"
                android:layout_marginLeft="@dimen/x20"
                />
            <TextView
                android:id="@+id/my_Warehouse_Datail_cangchufei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="￥300.00"
                android:textSize="@dimen/x24"
                android:textColor="@color/name_se"
                android:layout_marginLeft="@dimen/x50"
                />
        </LinearLayout>

    </LinearLayout>
    <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="@dimen/x16"-->
        <!--android:background="@color/fengouxian"-->
        <!--android:layout_marginTop="@dimen/x24"-->
        <!--/>-->


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/x16"
            >
            <Button
                android:id="@+id/my_Warehouse_Datail_zhuanzeng"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="@dimen/x70"
                android:text="转赠"
                android:textSize="@dimen/x28"
                android:textColor="@color/baise"
                android:layout_marginLeft="@dimen/x20"
                android:background="@drawable/congzhi_bg"
                android:gravity="center"
                />
            <Button
                android:id="@+id/my_Warehouse_Datail_tihuo"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="@dimen/x70"
                android:text="提货"
                android:textSize="@dimen/x28"
                android:textColor="@color/baise"
                android:layout_marginRight="@dimen/x20"
                android:background="@drawable/congzhi_bg"
                android:layout_marginLeft="@dimen/x16"
                android:gravity="center"
                />
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
