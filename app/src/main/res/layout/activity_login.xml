<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:test="http://schemas.android.com/apk/res/com.ifeng_tech.treasuryyitong"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ifeng_tech.treasuryyitong.ui.LoginActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/x50"
        >
        
        <TextView
            android:id="@+id/login_deng"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" 
            android:text="登录"
            android:gravity="end"
            android:textColor="@color/name_se"
            android:textSize="@dimen/x15"
            />
        <TextView
            android:layout_width="@dimen/x30"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/login_zhuce"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" 
            android:text="注册"
            android:textColor="@color/zhuse_ziti"
            android:textSize="@dimen/x15"
            />
    </LinearLayout>
    
    <FrameLayout
        android:id="@+id/login_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </FrameLayout>


    <!--<com.ifeng_tech.treasuryyitong.view.SlideUnlockView-->
        <!--android:id="@+id/slideUnlockView"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="@dimen/x20"-->
        <!--android:layout_centerInParent="true"-->
        <!--test:slideUnlockBackgroundResource="@mipmap/jiesuo_bg"-->
        <!--test:slideUnlockBlockResource="@mipmap/jiesuo_button" />-->
</LinearLayout>
