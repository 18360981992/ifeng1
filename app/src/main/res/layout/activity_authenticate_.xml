<?xml version="1.0" encoding="utf-8"?>
<!--托管鉴定 详情页-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/fuise_dabg"
    tools:context="com.ifeng_tech.treasuryyitong.ui.my.tuoguan.Authenticate_Details_Activity">

    <RelativeLayout
        android:id="@+id/authenticate_Details_Toobar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:background="@drawable/zichan_jianbian"
        >
        <RelativeLayout
            android:id="@+id/authenticate_Details_Fan"
            android:layout_width="@dimen/x60"
            android:layout_height="match_parent"
            android:background="@drawable/fanhui_bg"
            >
            <ImageView
                android:layout_width="@dimen/x16"
                android:layout_height="@dimen/x28"
                android:layout_alignParentBottom="true"
                android:background="@drawable/btn_back"
                android:layout_marginBottom="@dimen/x20"
                android:layout_marginLeft="@dimen/x30"
                />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="托管鉴定"
            android:textSize="@dimen/x32"
            android:textColor="@color/baise"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/x16"
            android:layout_centerHorizontal="true"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none"
            android:fitsSystemWindows="true">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/authenticate_Details_xiangqing"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x240"
                    android:layout_marginTop="@dimen/x30"
                    android:layout_marginRight="@dimen/x18"
                    android:layout_marginLeft="@dimen/x18"
                    android:paddingBottom="@dimen/x20"
                    android:background="@drawable/zhuanzeng_xiangqing"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:id="@+id/authenticate_Details_bianma"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="编号："
                        android:textSize="@dimen/x26"
                        android:textColor="@color/color_999999"
                        android:layout_marginLeft="@dimen/x30"
                        android:gravity="center_vertical"
                        />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x2"
                        android:background="@color/fengouxian"
                        android:layout_marginRight="@dimen/x85"
                        android:layout_marginLeft="@dimen/x30"
                        />
                    <TextView
                        android:id="@+id/authenticate_Details_name"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1.5"
                        android:text=""
                        android:textSize="@dimen/x28"
                        android:textColor="@color/name_se"
                        android:layout_marginLeft="@dimen/x30"
                        android:paddingBottom="@dimen/x35"
                        android:layout_marginRight="@dimen/x80"
                        android:gravity="center_vertical"
                        />

                    <!--<ImageView-->
                        <!--android:id="@+id/authenticate_Details_img"-->
                        <!--android:layout_width="@dimen/x130"-->
                        <!--android:layout_height="@dimen/x130"-->
                        <!--android:layout_marginLeft="@dimen/x25"-->
                        <!--android:layout_gravity="center_vertical"-->
                        <!--/>-->

                    <!--<LinearLayout-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:orientation="vertical"-->
                        <!--android:layout_gravity="center">-->

                        <!--<TextView-->
                            <!--android:id="@+id/authenticate_Details_bianma"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:layout_marginLeft="@dimen/x20"-->
                            <!--android:text=""-->
                            <!--android:textColor="@color/name_se"-->
                            <!--android:textSize="@dimen/x28" />-->

                        <!--<TextView-->
                            <!--android:id="@+id/authenticate_Details_name"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:layout_marginLeft="@dimen/x20"-->
                            <!--android:layout_marginTop="@dimen/x10"-->
                            <!--android:text=""-->
                            <!--android:textColor="@color/name_se"-->
                            <!--android:textSize="@dimen/x28" />-->
                    <!--</LinearLayout>-->

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x260"
                    android:orientation="vertical"
                    android:background="@color/baise"
                    android:layout_marginRight="@dimen/x18"
                    android:layout_marginLeft="@dimen/x18"
                    android:layout_marginTop="@dimen/x8"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="起征数量"
                                android:textSize="@dimen/x28"
                                android:textColor="@color/color_666666"

                                />
                            <TextView
                                android:id="@+id/authenticate_Details_shuliang"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textSize="@dimen/x26"
                                android:textColor="@color/name_se"
                                android:layout_marginTop="@dimen/x10"
                                />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="托管进度"
                                android:textSize="@dimen/x28"
                                android:textColor="@color/color_666666"
                                />
                            <TextView
                                android:id="@+id/authenticate_Details_jindu"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textSize="@dimen/x26"
                                android:textColor="@color/name_se"
                                android:layout_marginTop="@dimen/x10"
                                />
                        </LinearLayout>

                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/x2"
                        android:background="@color/fengouxian"
                        android:layout_marginRight="@dimen/x20"
                        android:layout_marginLeft="@dimen/x20"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/x15"
                        android:layout_marginTop="@dimen/x15"
                        >
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="计件单位"
                                android:textSize="@dimen/x26"
                                android:textColor="@color/color_666666"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/x20"
                                />
                            <TextView
                                android:id="@+id/authenticate_Details_danwei"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="枚"
                                android:textSize="@dimen/x26"
                                android:textColor="@color/name_se"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="@dimen/x20"
                                />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="申请时间"
                                android:textSize="@dimen/x26"
                                android:textColor="@color/color_666666"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/x20"
                                />
                            <TextView
                                android:id="@+id/authenticate_Details_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textSize="@dimen/x26"
                                android:textColor="@color/name_se"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:layout_marginRight="@dimen/x20"
                                />
                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x5"
                    android:background="@color/fengouxian"
                    android:layout_marginLeft="@dimen/x18"
                    android:layout_marginRight="@dimen/x18"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x80"
                    android:gravity="center_vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/x20"
                        android:text="预约时间"
                        android:textColor="@color/name_se"
                        android:textSize="@dimen/x30" />
                </LinearLayout>

                <com.ifeng_tech.treasuryyitong.view.MyListView
                    android:id="@+id/authenticate_Details_MyListView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/baise"
                    android:divider="@null"
                    android:overScrollMode="never"
                    ></com.ifeng_tech.treasuryyitong.view.MyListView>

            </LinearLayout>
        </ScrollView>

        <LinearLayout
            android:id="@+id/authenticate_Details_weitanchuan"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x60"
            android:background="@color/color_5576E2"
            android:gravity="center_vertical"
            android:visibility="invisible">

            <ImageView
                android:id="@+id/authenticate_Details_weitanchuan_img"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x40"
                android:layout_marginLeft="@dimen/x16" />

            <TextView
                android:id="@+id/authenticate_Details_weitanchuan_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x16"
                android:text="预约成功"
                android:textColor="@color/baise"
                android:textSize="@dimen/x28" />
        </LinearLayout>
    </RelativeLayout>


</LinearLayout>
